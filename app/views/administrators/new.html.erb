<%= form_for @administrator, :url => { :controller => 'administrators', :action => 'create' } do |f| %>
    <%= f.label :email, t("view.email") %>
    <%= f.email_field :email %>
    <br>

    <%= f.label :password, t("view.password") %>
    <%= f.password_field :password %>
    <br>

    <%= f.label :password_confirm, t("view.password_confirm") %>
    <%= f.password_field :password_confirm %>
    <br>
    <%=  f.select(:administrator_type_id, options_for_select(
         @administrator_types.map{|i| [t("db_type.administrator_type.#{i.name}") , i.id]})) %>
    <%=%>
    <br>

    <%= f.submit t("view.send") %>
    <br>
    <% a="master" %>
<% end %>

