<%=  form_for :member, :url =>{:controller => "members",:action => "update"}  do |f|%>
<%= f.label :nickname, t("view.nickname") %>
<%= f.text_field :nickname %>
<br>
<%=f.label :password, t("view.password")%>
<%=f.password_field :password %>
<br>
<%= f.label :password_confirm, t("view.password_confirm")%>
<%= f.password_field :password_confirm %>
<%=%>
<br>
<%= f.submit t("view.update")%>
<% end %>
